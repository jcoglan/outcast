<%= @jsonp_callback %>({
  "artists":  <%= @library.artists.entries.inspect %>,
  "albums":   <%= @library.albums.entries.inspect %>,
  
  "tracks": [
    <% @library.entries.each_with_index do |track, i| %>
      {
        "artist":   "<%= CGI.escape track[Outcast::ITunes::ARTIST] %>",
        "album":    "<%= CGI.escape track[Outcast::ITunes::ALBUM] %>",
        "name":     "<%= CGI.escape track[Outcast::ITunes::NAME] %>",
        "path":     "<%= CGI.escape track[Outcast::ITunes::LOCATION] %>"
      }<%= i == @library.size - 1 ? '' : ',' %>
    <% end %>
  ]
});
